<AbsoluteLayout class="shadeCover">
    <FlexboxLayout justifyContent="space-around" alignItems="center" width="100%" height="100%">
        <GridLayout rows="auto,auto,auto" columns="auto,auto" class="modal-dialog">
            <Label row="0" col="0" colSpan="2" text="Select time" class="object-detail-subtitle" />
            <TimePicker row="1" col="0" colSpan="2" time="{{ addAck.expireTime }}" id="timePicker" />
            <Button row="2" col="0" text="Ok" tap="onTimePickerOk" />
            <Button row="2" col="1" text="Cancel" tap="onTimePickerCancel" />
        </GridLayout>
    </FlexboxLayout>
</AbsoluteLayout>