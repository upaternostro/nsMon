<StackLayout orientation="horizontal">
    <Button text="{{ (!object.isNotAcknowledged ? 'Remove ' : '') + 'Ack' }}" isEnabled="{{ !object.isOk }}" tap="onAckTap" />
    <Button text="Check" tap="onCheckTap" />
    <Button text="Comment" tap="onCommentTap"  />
    <Button text="Notify" tap="onNotificationTap"  />
</StackLayout>
