<StackLayout orientation="horizontal">
    <Button text="{{ (object.attrs.acknowledgement != 0 ? 'Remove ' : '') + 'Ack' }}" isEnabled="{{ object.attrs.state != 0 }}" tap="onAckTap" />
    <Button text="Check" tap="onCheckTap" />
    <Button text="Comment" tap="onCommentTap"  />
    <Button text="Notify" />
</StackLayout>
